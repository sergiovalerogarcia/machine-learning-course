{
  "build": { "dockerfile": "../Dockerfile" },
  "runArgs": [
    "--network", "host",
    "--env-file",".env.properties",
    "--env-file",".env.secrets",
    "-e JUPYTER_ENABLE_LAB=yes",
  ],
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/jupyuser/${localWorkspaceFolderBasename},type=bind,consistency=delegated",
  "workspaceFolder": "/home/jupyuser/${localWorkspaceFolderBasename}",
  "postCreateCommand": "pip freeze > requirements.txt"
}